@model StudentViewModel


<div class="container">
    <h5>The current applicants are awaiting approval or rejection</h5>
    <div class="row">
        <table class="table table-striped table-dark m-2">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">First</th>
                    <th scope="col">Last</th>
                    <th scope="col">Predicted Grade</th>
                    <th scope="col">Actual Grade</th>
                    <th scope="col">Mission Statement</th>
                    <th scope="col">Accept</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var student in Model.Students)
                {
                    <tr>
                        <th scope="row">@student.StudentId</th>
                        <td>@student.FirstName</td>
                        <td>@student.LastName</td>
                        <td>@student.PredictedGrade</td>
                        <td>@student.ActualGrade</td>
                        <td>@student.MissionStatement</td>
                        <td> Y / N</td>
                    </tr>
                }


            </tbody>
        </table>
    </div>
</div>





